{
  "name": "rag-sdk",
  "version": "1.0.0",
  "description": "TypeScript/JavaScript SDK fÃ¼r Retrieval-Augmented Generation (RAG) Services basierend auf Azure OpenAI",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "generate": "orval",
    "build": "tsc",
    "prepare": "npm run build",
    "clean": "rm -rf dist coverage",
    "prebuild": "npm run clean",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "lint": "tsc --noEmit",
    "lint:strict": "tsc --noEmit --strict",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "check-format": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\""
  },
  "keywords": [
    "rag",
    "retrieval-augmented-generation",
    "ai",
    "nlp",
    "embeddings",
    "completion",
    "sdk",
    "typescript",
    "javascript",
    "azure",
    "openai",
    "microsoft"
  ],
  "author": "Your Name",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/rag-sdk.git"
  },
  "dependencies": {
    "@azure/identity": "^4.11.1",
    "@azure/openai": "^1.0.0-beta.12",
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "jest": "^29.7.0",
    "nock": "^13.3.0",
    "orval": "^6.15.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
